a	B.cpp	/^char a[3001],b[3001];$/;"	v
b	B.cpp	/^char a[3001],b[3001];$/;"	v
la	B.cpp	/^int la,lb;$/;"	v
lb	B.cpp	/^int la,lb;$/;"	v
main	B.cpp	/^int main()$/;"	f
